 [Unit]
 Description=GreenSense 1602 LCD Shield System UI Service
 After=multi-user.target

 [Service]
 Type=idle
 ExecStart=/bin/bash -c "mkdir -p /usr/local/Serial1602ShieldSystemUIController/; cd /usr/local/Serial1602ShieldSystemUIController/; bash start-ui-controller-from-github.sh master --SerialPort=/dev/ttyUSB0 --DeviceName ui1 --DevicesDirectory=/usr/local/GreenSense/Index/devices"

 [Install]
 WantedBy=multi-user.target
